{
  "name": "bitgud",
  "displayName": "bitgud ",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:bitgud.insult",
    "onCommand:bitgud.authenticate",
    "onView:bitgud-view"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "bitgud.insult",
        "title": "Insult"
      },
      {
        "command": "bitgud.authenticate",
        "title": "Auth"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "bitgud-container",
          "title": "Bitgud",
          "icon": "BitGud.svg"
        }
      ]
    },
    "views": {
      "bitgud-container": [
        {
          "id": "bitgud-view",
          "name": "BitGud",
          "icon": "BitGud.svg",
          "contextualTitle": "LogIn"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.58.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "@octokit/rest": "^18.7.2",
    "axios": "^0.21.1",
    "express": "^4.17.1"
  }
}
